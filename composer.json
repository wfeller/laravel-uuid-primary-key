{
    "name": "wfeller/laravel-uuid-primary-key",
    "description": "Trait to use ordered uuids on your models.",
    "keywords": ["Laravel", "uuid"],
    "homepage": "https://github.com/wfeller/laravel-uuid-primary-key",
    "license": "MIT",
    "authors": [
        {
            "name": "William",
            "homepage": "https://github.com/wfeller"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "~5.7.0",
        "moontoast/math": "^1.1"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "WF\\UuidPrimaryKey\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WF\\UuidPrimaryKey\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
